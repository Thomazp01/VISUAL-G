Var
// Seção de Declarações das variáveis
continuar: caractere
x: inteiro

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...

 continuar <- "S"
 x <- 1
  enquanto (continuar = "S") faca
   escreval("Número = " , x )
   x <- x + 1
   escreval("Deseja digitar mais um número ? [S/n]")
   leia (continuar)

   fimenquanto



